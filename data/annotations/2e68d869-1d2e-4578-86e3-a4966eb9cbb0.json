      "uuid": "e1767cf1-f9e6-48dc-b6b5-a2b55582a6d5"
    },
    {
      "start_time": 9,
      "end_time": 11,
      "annotation": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "And yet each of us has awakened or risen"
            }
          ]
        }
      ],
      "tags": [
        {
          "tag": "Speech",
          "category": "Extra-Poetic Sounds"
        },
        {
          "tag": "Tempo",
          "category": "Performance"
        },
