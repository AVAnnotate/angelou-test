    {
      "start_time": 42,
      "end_time": 44,
      "annotation": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "You may write me down in history,"
            }
          ]
        }
      ],
      "tags": [
        {
          "tag": "Tempo",
          "category": "Performance"
        },
        {
          "tag": "Transcription",
          "category": "Background"
