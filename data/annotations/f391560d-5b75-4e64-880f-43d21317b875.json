      "annotation": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "Everyone in the world is going to bed one night or another with fear or pain or loss or disappointment"
            }
          ]
        }
      ],
      "tags": [
        {
          "tag": "Speech",
          "category": "Extra-Poetic Sounds"
        },
        {
          "tag": "Tempo",
          "category": "Performance"
